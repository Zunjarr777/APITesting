    agent any

    stages {
        stage('First_Branch') {
            steps {
                echo 'Hello World1'
            }
        }
        stage('Second_Branch') {
            steps {
                echo 'Hello World2'
            }
        }
        stage('Third_Branch') {
            steps {
                echo 'Hello World3'
            }
        }
        stage('Fourth_Branch') {
            steps {
                echo 'Hello World3'
            }
        }
    }
}
